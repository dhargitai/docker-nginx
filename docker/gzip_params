##
# Gzip Settings
##

gzip on;
gzip_vary on;
gzip_static on;
gzip_proxied any;
gzip_comp_level 5;
gzip_buffers 16 8k;
gzip_min_length 256;
gzip_disable "msie6";
gzip_http_version 1.1;
gzip_types text/plain text/css text/xml text/javascript application/x-javascript application/javascript application/json application/xml application/xml+rss;
